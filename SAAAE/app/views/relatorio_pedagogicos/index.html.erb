<%- model_class = RelatorioPedagogico -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:rpe_objetivo) %></th>
      <th><%= model_class.human_attribute_name(:rpe_atendimento) %></th>
      <th><%= model_class.human_attribute_name(:relatorio_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @relatorio_pedagogicos.each do |relatorio_pedagogico| %>
      <tr>
        <td><%= link_to relatorio_pedagogico.id, relatorio_pedagogico_path(relatorio_pedagogico) %></td>
        <td><%= relatorio_pedagogico.rpe_objetivo %></td>
        <td><%= relatorio_pedagogico.rpe_atendimento %></td>
        <td><%= relatorio_pedagogico.relatorio_id %></td>
        <td><%=l relatorio_pedagogico.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      relatorio_pedagogico_path(relatorio_pedagogico), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_relatorio_pedagogico_path(relatorio_pedagogico), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      relatorio_pedagogico_path(relatorio_pedagogico),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_relatorio_pedagogico_path,
            :class => 'btn btn-primary' %>
