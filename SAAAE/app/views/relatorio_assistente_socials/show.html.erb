<%- model_class = RelatorioAssistenteSocial -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ras_motivo) %>:</strong></dt>
  <dd><%= @relatorio_assistente_social.ras_motivo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ras_entrevista) %>:</strong></dt>
  <dd><%= @relatorio_assistente_social.ras_entrevista %></dd>
  <dt><strong><%= model_class.human_attribute_name(:relatorio_id) %>:</strong></dt>
  <dd><%= @relatorio_assistente_social.relatorio_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              relatorio_assistente_socials_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_relatorio_assistente_social_path(@relatorio_assistente_social), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              relatorio_assistente_social_path(@relatorio_assistente_social),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
