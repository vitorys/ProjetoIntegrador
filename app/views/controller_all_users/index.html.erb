<p id="notice"><%=notice%></p>
<div class="page-header">
  <h1>Users</h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Cargo</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
      </tr>
    <% end %>
  </tbody>
</table>
